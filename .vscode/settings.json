{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".next": true,
    ".vscode": true,
    "LICENSE": true,
    "next-env.d.ts": true,
    "node_modules": true,
    "package-lock.json": true,
    "postcss.config.js": true,
    "README.md": true,
    "js": true,
    ".vercel": true,
    "dist": true,
    "out": true,
    "tsconfig.json": true,
    "video.d.ts": true,
    "app/globals.css": true,
    "app/satoshi.css": true,
    "app/page.tsx": true,
    "app/fonts": true
  },
  "hide-files.files": [
    "tailwind.config.js",
    "tsconfig.json",
    "video.d.ts",
    "app/globals.css",
    "app/satoshi.css",
    "next.config.mjs",
    "app/css",
    ".eslintrc.json",
    "app/page.tsx",
    "components/Header",
    "app/fonts",
    "components"
  ],
  "conventionalCommits.scopes": [
    "global",
    "stock ticker",
    "news-feed"
  ],
  "cSpell.words": [
    "autosize",
    "boxdark",
    "strokedark"
  ],
  "cSpell.ignoreWords": [
    "drilldown",
    "inco"
  ]
}