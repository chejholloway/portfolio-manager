    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".eslintrc.json": true,
    ".next": true,
    ".vscode": true,
    "LICENSE": true,
    "next-env.d.ts": true,
    "node_modules": true,
    "package-lock.json": true,
    "postcss.config.js": true,
    "README.md": true,
